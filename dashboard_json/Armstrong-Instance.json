{
    "timeGrain": "hour",
    "cards": [
        {
            "id": "pump-summary-table",
            "size": "XLARGE",
            "title": "Pump Alert Summary",
            "type": "TABLE",
            "content": {
                "columns": [
                    {
                        "dataSourceId": "deviceid",
                        "label": "Device ID"
                    },
                    {
                        "dataSourceId": "timestamp",
                        "label": "Timestamp",
                        "type": "TIMESTAMP"
                    },
                    {
                        "dataSourceId": "Current",
                        "label": "Current Amps"
                    },
                    {
                        "dataSourceId": "Flow",
                        "label": "Flow Gallons Per Minute"
                    },
                    {
                        "dataSourceId": "Voltage",
                        "label": "Voltage KW"
                    },
                    {
                        "dataSourceId": "power",
                        "label": "Power HP"
                    }
                ],
                "thresholds": [
                    {
                        "dataSourceId": "Current",
                        "comparison": ">=",
                        "severity": 1,
                        "value": 4,
                        "label": "Severity"
                    }
                ],
                "sort": "DESC"
            },
            "dataSource": {
                "attributes": [
                    {
                        "aggregator": "mean",
                        "attribute": "Current",
                        "id": "Current"
                    },
                    {
                        "aggregator": "last",
                        "attribute": "Flow",
                        "id": "Flow"
                    },
                    {
                        "aggregator": "last",
                        "attribute": "Voltage",
                        "id": "Voltage"
                    },
                    {
                        "aggregator": "last",
                        "attribute": "power",
                        "id": "power"
                    }
                ],
                "groupBy": [
                    "deviceid"
                ]
            }
        },
        {
            "content": {
                "alt": "Generation 5 Pump Health",
                "zoomMax": 1,
                "hotspots": [
                    {
                        "color": "#FFF",
                        "content": {
                            "attributes": [
                                {
                                    "dataSourceId": "warn",
                                    "label": "Warning Alert"
                                }
                            ]
                        },
                        "locations": [
                            {
                                "xDataSourceId": "alarm",
                                "yDataSourceId": "warn",
                                "dataFilter": {
                                    "deviceid": "device_id"
                                }
                            }
                        ],
                        "thresholds": [
                            {
                                "color": "green",
                                "comparison": "=",
                                "dataSourceId": "warn",
                                "icon": "icon--close--solid",
                                "isHotspot": true,
                                "value": 0
                            },
                            {
                                "color": "yellow",
                                "comparison": ">",
                                "dataSourceId": "warn",
                                "icon": "icon--close--solid",
                                "isHotspot": true,
                                "value": 0
                            }
                        ]
                    },
                    {
                        "color": "#FFF",
                        "content": {
                            "attributes": [
                                {
                                    "dataSourceId": "alarm",
                                    "label": "Alarm Alert"
                                }
                            ]
                        },
                        "locations": [
                            {
                                "xDataSourceId": "warn",
                                "yDataSourceId": "alarm",
                                "dataFilter": {
                                    "deviceid": "device_id"
                                }
                            }
                        ],
                        "thresholds": [
                            {
                                "color": "green",
                                "comparison": "=",
                                "dataSourceId": "alarm",
                                "icon": "icon--close--solid",
                                "isHotspot": true,
                                "value": 0
                            },
                            {
                                "color": "red",
                                "comparison": ">",
                                "dataSourceId": "alarm",
                                "icon": "icon--close--solid",
                                "isHotspot": true,
                                "value": 0
                            }
                        ]
                    }
                ],
                "image": "pump5"
            },
            "dataSource": {
                "attributes": [
                    {
                        "attribute": "alarm",
                        "id": "alarm"
                    },
                    {
                        "attribute": "warn",
                        "id": "warn"
                    }
                ],
                "groupBy": [
                    "deviceid"
                ]
            },
            "id": "pump5",
            "size": "LARGE",
            "title": "Pump Alerts",
            "type": "IMAGE"
